<section class="js-masonry">
	<div id="contenido">
	  <ul>
	    <li style="text-align:center" class="box">
        <script>
          google_ad_client = "ca-pub-3074485176185204";
          google_ad_slot = "5370019667"; 
          google_ad_width = 250; 
          google_ad_height = 250; 
        </script>
        <script src="http://pagead2.googlesyndication.com/pagead/show_ads.js"></script>
        <!-- Google AdSense-->
	    </li>

	    <% _.each(post, function(post){ %>
	    <li class="box">
	    	<% if ( post.photos) { %>
	    	<a href="/post/<%= post.id %>">
	    		<img src="<%=post.photos %>" alt="<%= post.title %>">
	    	</a>
        <% } %>
	    	<h4><a href="/post/<%= post.id %>"><%= post.title %></a></h4>
	      	<p><%= post.summary %></p>
          <p>
              <i class="glyphicon glyphicon-heart"></i>   <span> 5 </span>
              <i class="glyphicon glyphicon-search"></i>  <span> 7 </span>
              <i class="glyphicon glyphicon-star"></i>    <span> 2 </span>
              <i class="glyphicon glyphicon-flag"></i>    <span> 20 </span>
          </p>

          <p>
              <i class="icon-tags"></i> Tags: 
              <%
              if(post.populars) { var str = post.populars; } else { var str = post.title; }
              var arr = str.replace(/,/g," ").split(' ');
              for(var i=0;i<arr.length;i++) { 
              %>
              <a class="tagsbypost" href="/post/tags/<%= arr[i] %>" target="_parent">
                <%= arr[i] %>
              </a>
              <% } %>
          </p>
	    </li>
	    <% }); %>

	    <li style="text-align:center" class="box">
        <script>
            google_ad_client = "ca-pub-3074485176185204";
            google_ad_slot = "5370019667"; 
            google_ad_width = 250; 
            google_ad_height = 250; 
        </script>
        <script src="http://pagead2.googlesyndication.com/pagead/show_ads.js"></script>
        <!-- Google AdSense-->
	    </li>
	  </ul>
	</div>
</section>


