    <div id="wrap">
      <div class="navbar-fixed-top">
        <header>
          <div id="logo">
            <a href="/">
              <img src="/img/logo.png" alt="logo" width="166" height="73">
            </a>
          </div>
          <div>
            <a href="#" id="dondeestoy">
              <div></div>
            </a>
          </div>
          <nav>
            <ul>
              <li>
                <a href="/">
                  <div><i class="glyphicon glyphicon-home"></i></div>
                </a>
              </li>
              <li>
                <a href="<% if( loggedUser ) { %>/user/<%= loggedUser.id %><% } else { %>javascript:login();<% }%>">
                  <div id="quiensoy">
                    <i class="icon-user"></i><span class="hidemobile showhover"> <% if( loggedUser ) { %><%= loggedUser.username %><% } else { %><%= __('Place your ad for free') %><% }%></span>
                  </div>
                </a>
              </li>
              <li id="ocultar">
                <a href="/map" id="ubicacion">
                  <div><i class="glyphicon glyphicon-globe"></i></div>
                </a>
              </li>
              <li class="hidemobile">
                  <a href="javascript:showMenu();" id="busqueda">
                    <div><i class="icon-search"></i></div>
                  </a>
              </li>
              <li>
                <a href="javascript:menu();" id="elmenu">
                  <div><i class="icon-profile"></i></div>
                </a>
              </li>
            </ul>
          </nav>
          
        </header>
        <div id="buscador" class="row">
          <div id="lacaja" class="col-xs-12 col-sm-6">
            <form action="/post/search/">
              <div id="searchbox">
                <input type="type" id="term" name="term" placeholder="<%= __('Start your search here') %>" class="form-control" />
              </div>
              <div>  
                <input type="submit" value="Go" class="btn btn-default">
              </div>     
            </form>
          </div>
          <div id="popular" class="col-md-6">
            <ul>
              <li class="tag4'"><a href="#">Loading populars tags</a></li>
            </ul>
          </div>
        </div>
      </div>

      <!-- content -->
      <%- body %>
      <!-- content -->      

      <footer id="footer" class="smooth-style slideOut-option navbar-fixed-bottom">
        <div class="container">
          <div>2013-2014 &copy <a href="http://www.loultimoenlaweb.com" target="_blank">Lo Ultimo Group</a> & <a href="http://www.danielnaranjo.info" target="_blank">Daniel Naranjo</a></div>
          <div>Follow us in <a href="http://linkedin.com/company/findby" target="_blank">Linkedin</a>, <a href="http://facebook.com/findby" target="_blank">Facebook</a>, <a href="http://twitter.com/findbyco" target="_blank">Twitter</a>  and <a href="https://plus.google.com/115826745735747307118" rel="publisher">Google+</a> |  <a href="/about/contact"><%= __('Contact')%></a> - <a href="/about/privacy"><%= __('Privacy')%></a> - <a href="/about/legal"><%= __('Legal')%></a>
          </div>
        </div>
      </footer>
    </div>

    <div id="opciones">
      <ul>
        <% if( loggedUser ) { %><li><a href="/user/<%= loggedUser.id %>"><%= __('My Profile') %></a></li><% } else { %>
        <li><a href="javascript:newOne();"><%= __('Sign Up') %></a></li>
        <li><a href="javascript:login();"><%= __('Log In') %></a></li>
      <% } %>        
        <% if( loggedUser ) { %><li><a href="/post/new"><%= __('Create a new post') %></a></li><% } else { %><li><a href="/post"><%= __('All Posts') %></a></li><% } %>
        <li><a href="/map"><%= __('Explorer')%></a></li>
        <li><a href="/about"><%= __('About')%></a></li>
        <li><a href="http://findby.uservoice.com" target="_blank"><%= __('Faq')%></a></li>
        <li><a href="/about/privacy"><%= __('Privacy')%></a></li>
        <li><a href="/about/contact"><%= __('Contact')%></a></li>
        <% if( loggedUser ) { %>
        <li>&nbsp;</li>
        <li><a href="/user/logout"><%= __('Sign Out')%></a></li><% } %>
      </ul>
    </div>

    <div id="nogeo"></div>

