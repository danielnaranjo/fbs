  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>FindBY - A Geolocation Classified Ads for products and services around the world</title>
    <!-- Viewport mobile tag for sensible mobile support -->
    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1,user-scalable=no">
    <meta name="description" content="A Geolocation Classified Ads for products and services around the world">
    <meta name="author" content="Daniel Naranjo">
    <link rel="icon" href="/favicon.ico">    
    <!--STYLES-->
    <link rel="stylesheet" href="/linker/styles/bootstrap.css">
    <link rel="stylesheet" href="/linker/styles/bootstrap-theme.css">
    <link rel="stylesheet" href="/linker/styles/animate-custom.css">
    <link rel="stylesheet" href="/linker/styles/bootstrap-social.css">
    <link rel="stylesheet" href="/linker/styles/estilos.css">
    <!--STYLES END-->
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
    <script src="http://code.jquery.com/jquery-1.11.0.min.js"></script>
  </head>

  <body>
  <!-- Prompt IE 6 users to install Chrome Frame. Remove this if you support IE 6.
       chromium.org/developers/how-tos/chrome-frame-getting-started -->
  <!--[if lt IE 7]><p class=chromeframe>Your browser is <em>ancient!</em> <a href="http://browsehappy.com/">Upgrade to a different browser</a> or <a href="http://www.google.com/chromeframe/?redirect=true">install Google Chrome Frame</a> to experience this site.</p><![endif]-->    
    <div id="wrap">
      <div class="navbar-fixed-top">
        <header>
          <div id="logo">
            <a href="/">
              <img src="/img/logo.png" alt="logo">
            </a>
          </div>
          <div id="busqueda"><i class="icon-search"></i></div>
          <div id="ubicacion"><i class="icon-location"></i></div>
          <div id="elmenu"><i class="icon-profile"></i></div>
          <div id="dondeestoy"></div>
          <div id="quiensoy">
            <i class="icon-user"></i> <% if( loggedUser ) { %><a href="/user/<%= loggedUser.id %>"><%= loggedUser.username %></a><% } else { %><a href="/user/new"><%= __('Sign Up') %> / <%= __('Log In') %></a><% }%>
          </div>
        </header>
        <div id="buscador" class="row">
          <div id="lacaja" class="col-md-6">
            <form action="/post/search/">
              <div>
                <input type="type" id="term" name="term" placeholder="<%= __('Start your search here') %>" class="form-control" />
              </div>
              <div>  
                <input type="submit" value="Go" class="btn btn-default">
              </div>     
            </form>
          </div>
          <div id="popular" class="col-md-6">
            <ul>
              <li class="tag4'"><a href="#">Loading populars tags</a></li>
            </ul>
          </div>
        </div>
      </div>

      <!-- content -->
      <%- body %>
      <!-- content -->      

      <footer id="footer" class="smooth-style slideOut-option navbar-fixed-bottom">
        <div class="container">
          <div>2013-2014 &copy  <a href="http://www.danielnaranjo.info">Daniel Naranjo</a></div>
          <div>Follow us in <a href="http://facebook.com/findby" target="_blank">Facebook</a> or <a href="http://twitter.com/findbytags" target="_blank">Twitter</a>  |  <a href="/about/contact"><%= __('Contact')%></a></div>
        </div>
      </footer>
    </div>

    <div id="opciones">
      <ul>
        <% if( loggedUser ) { %><li><a href="/user/<%= loggedUser.id %>"><%= __('My Profile') %></a></li><% } else { %>
        <li><a href="/user/new"><%= __('Sign Up') %></a></li>
        <li><a href="/user/auth"><%= __('Log In') %></a></li>
      <% } %>        
        <% if( loggedUser ) { %><li><a href="/post/new"><%= __('Create a new post') %></a></li><% } else { %><li><a href="/post"><%= __('All Posts') %></a></li><% } %>
        <li><a href="/map"><%= __('Explorer')%></a></li>
        <li><a href="/about"><%= __('About')%></a></li>
        <li><a href="http://findby.uservoice.com" target="_blank"><%= __('Faq')%></a></li>
        <li><a href="/about/privacy"><%= __('Privacy')%></a></li>
        <li><a href="/about/contact"><%= __('Contact')%></a></li>
        <% if( loggedUser ) { %>
        <li>&nbsp;</li>
        <li><a href="/user/logout"><%= __('Sign Out')%></a></li><% } %>
      </ul>
    </div>

    <div id="nogeo"></div>

    <!--TEMPLATES-->
    
    <!--TEMPLATES END-->


    </script>
    <!--SCRIPTS-->
    <script src="/linker/js/socket.io.js"></script>
